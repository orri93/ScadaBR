        </description>
        <body-content>empty</body-content>
        <attribute>
            <description>HTML Standard Attribute</description>
            <name>id</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>HTML Standard Attribute</description>
            <name>name</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>text</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>i18nLabel</name>
            <required>true</required>
        </attribute>
        <attribute>
            <name>i18nTitle</name>
        </attribute>
    </tag>
    
    <tag>
        <name>numberSpinner</name>
        <tag-class>br.org.scadabr.web.taglib.dijit.NumberSpinnerTag</tag-class>
        <description>
