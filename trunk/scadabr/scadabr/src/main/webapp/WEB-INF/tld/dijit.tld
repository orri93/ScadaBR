<?xml version="1.0" encoding="UTF-8"?>

<taglib xmlns="http://java.sun.com/xml/ns/j2ee" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
        xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd" 
        version="2.0">
    <tlib-version>1.0</tlib-version>
    <jsp-version>1.2</jsp-version>
    <description>
        Custom tag library for dijit widgets
    </description>
    <short-name>dijit</short-name>

    <tag>
        <name>dateTextBox</name>
        <tag-class>br.org.scadabr.web.taglib.dijit.DateTextBoxTag</tag-class>
        <description>
            Writes a input tag for dijit/form/DateTextBox.
        </description>
        <body-content>empty</body-content>
        <attribute>
            <description>HTML Standard Attribute</description>
            <name>id</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>HTML Standard Attribute</description>
            <name>name</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>HTML Standard Attribute</description>
            <name>timestamp</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>i18nLabel</name>
            <required>true</required>
        </attribute>
        <attribute>
            <name>i18nTitle</name>
        </attribute>
    </tag>
  
      
    <tag>
        <name>textarea</name>
        <tag-class>br.org.scadabr.web.taglib.dijit.TextareaTag</tag-class>
        <description>
