    <tag>
        <name>validationTextBox</name>
        <description>
            Writes a input tag for dijit/form/ValidatingTextBox.
        </description>
        <tag-class>br.org.scadabr.web.taglib.dijit.ValidationTextBoxTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>HTML Standard Attribute</description>
            <name>name</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>value</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>HTML Standard Attribute</description>
            <name>id</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>i18nLabel</name>
            <required>true</required>
        </attribute>
        <attribute>
            <name>i18nTitle</name>
        </attribute>
    </tag>

    <tag>
        <name>loginTextBox</name>
        <description>
            Writes initially a input tag for dijit/form/TextBox or if binding errors exists a dijit/form/ValidatingTextBox. 
            To assist the browser filling in the username ad password.
        </description>
        <tag-class>br.org.scadabr.web.taglib.dijit.LoginTextBoxTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Path to property for data binding</description>
            <name>path</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>HTML Standard Attribute</description>
            <name>id</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>i18nLabel</name>
            <required>true</required>
        </attribute>
        <attribute>
            <name>i18nTitle</name>
        </attribute>
        <body-content>empty</body-content>
        <dynamic-attributes>true</dynamic-attributes>
    </tag>
    <tag>
        <name>button</name>
        <tag-class>br.org.scadabr.web.taglib.dijit.ButtonTag</tag-class>
        <description>
            Writes a button tag for dijit/form/Button.
        </description>
        <body-content>JSP</body-content>
        <attribute>
            <name>id</name>
        </attribute>
        <attribute>
            <name>type</name>
        </attribute>
        <attribute>
            <name>name</name>
        </attribute>
        <attribute>
            <name>iconClass</name>
        </attribute>
        <attribute>
            <name>i18nLabel</name>
        </attribute>
        <attribute>
            <name>i18nTitle</name>
        </attribute>
        <attribute>
            <name>disabled</name>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>toolbar</name>
        <tag-class>br.org.scadabr.web.taglib.dijit.ToolbarTag</tag-class>
        <description>
